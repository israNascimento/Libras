<html>
    <head>
        <title>Libras</title>
        <meta charset="utf-8">
    </head>
    <body>
        <nav>
            <ul>
                <%
                    var disciplinas = ["Portugues", "Matematica", "Historia", "Geografia", "Fisica", "Quimica"];
                    disciplinas.forEach((value) => { 
                        const link = "/disciplinas/"+value; %>
                        <li><a href='<%= link  %>'><%= value %></a></li>
                 <% });
                %>
            </ul>
        </nav>